{
  "bindings": [
    {
      "name": "Timer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 0 * * Sun"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "cosmos-spotify-use2-prod",
      "collectionName": "recentlyPlayed",
      "createIfNotExists": false,
      "connectionStringSetting": "COSMOS_DB_CS",
      "partitionKey": "id"
    }
  ]
}